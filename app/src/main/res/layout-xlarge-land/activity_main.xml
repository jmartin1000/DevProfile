<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="75dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/banner"
            android:scaleType="fitCenter"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:background="#232323" android:contentDescription="TODO"/>
    <ImageView
            android:layout_width="75dp"
            android:layout_height="75dp" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:layout_marginTop="60dp"
            app:layout_constraintTop_toTopOf="parent" android:contentDescription="TODO"/>
    <TextView
            android:text="@string/devslopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/logo"
            android:layout_marginStart="24dp" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/banner"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView10"
            android:contentDescription="TODO"
            app:layout_constraintStart_toEndOf="@+id/textView"
            app:layout_constraintHorizontal_bias="0.9" app:layout_constraintEnd_toStartOf="@+id/imageView11"
            app:layout_constraintHorizontal_chainStyle="packed" app:layout_constraintTop_toTopOf="@+id/imageView11"
            app:layout_constraintBottom_toBottomOf="@+id/imageView11"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView11"
            android:contentDescription="TODO"
            app:layout_constraintStart_toEndOf="@+id/imageView10"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView12"
            android:layout_marginStart="24dp" android:layout_marginEnd="24dp"
            app:layout_constraintTop_toTopOf="@+id/imageView12"
            app:layout_constraintBottom_toBottomOf="@+id/imageView12"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView12"
            android:contentDescription="TODO"
            app:layout_constraintStart_toEndOf="@+id/imageView11" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/banner"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline2"
                                          android:orientation="vertical" app:layout_constraintGuide_percent="0.5"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView14"
            android:scaleType="centerCrop"
            app:layout_constraintDimensionRatio="h,1:1"
            android:contentDescription="TODO"
            app:layout_constraintTop_toTopOf="@+id/imageView16"
            app:layout_constraintBottom_toBottomOf="@+id/imageView16"
            app:layout_constraintStart_toEndOf="@+id/imageView16" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView15"
            android:scaleType="centerCrop"
            app:layout_constraintDimensionRatio="1:1"
            android:contentDescription="TODO"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView17"
            app:layout_constraintTop_toBottomOf="@+id/logo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_chainStyle="packed" android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView16"
            android:scaleType="centerCrop"
            app:layout_constraintDimensionRatio="1:1"
            android:contentDescription="TODO"
            app:layout_constraintTop_toTopOf="@+id/imageView17"
            app:layout_constraintBottom_toBottomOf="@+id/imageView17"
            app:layout_constraintStart_toEndOf="@+id/imageView17" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView14" app:layout_constraintVertical_bias="0.545"
            android:layout_marginStart="8dp" android:layout_marginEnd="16dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView17"
            android:scaleType="centerCrop"
            android:contentDescription="TODO"
            app:layout_constraintTop_toTopOf="@+id/imageView15"
            app:layout_constraintBottom_toBottomOf="@+id/imageView15"
            app:layout_constraintStart_toEndOf="@+id/imageView15" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView16" app:layout_constraintDimensionRatio="1:1"
            android:layout_marginStart="16dp" android:layout_marginEnd="8dp"/>
</android.support.constraint.ConstraintLayout>